  },
  {
    "object": "block",
    "type": "paragraph",
    "paragraph": {
      "rich_text": [
        {
          "type": "text",
          "text": {
            "content": "Start session outside the shell, then walk into the shell with app camera active. Go 3-4 meters inside shell, turn around, aim crosshair at bottom of root, and tap \"Place Origin\". Three colored axes and blade model will appear."
          }
        }
      ]
    }
